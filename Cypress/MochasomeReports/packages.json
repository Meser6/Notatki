{
  "name": "testy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean:reports": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports", #usuwa stare raporty
    "pretest": "npm run clean:reports",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json", #laczy pliki json z kazdego feature
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports", #genetuje raport
    "posttest": "npm run combine-reports && npm run generate-report",
    "cy:run": "(npm run pretest && npx cypress run --spec \"cypress/e2e/**/*\") || npm run posttest" #odpala testy i generuje wszystko
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cypress": "^12.3.0",
    "cypress-multi-reporters": "^1.6.1",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0"
  }
}